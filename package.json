{
  "name": "express-react-mysql-boilerplate",
  "description": "Basic boilerplate for react express and mysql based applications",
  "version": "0.0.1",
  "homepage": "https://github.com/vijay-j/react-express-mysql-boilerplate/",
  "babel": {
    "presets": [
      "react",
      "es2015",
      "stage-3"
    ],
    "env": {
      "development": {
        "plugins": [
          "babel-plugin-rewire"
        ]
      },
      "production": {
        "plugins": []
      }
    }
  },
  "author": {
    "name": "Vijay Jandhyala",
    "email": "jvs.vijaykumar@gmail.com",
    "homepage": "https://github.com/vijay-j"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vijay-j/react-express-mysql-boilerplate.git"
  },
  "engines": {
    "node": "0.12.7"
  },
  "dependencies": {
    "json5": "0.5.1",
    "babel": "6.23.0",
    "babel-core": "6.25.0",
    "babel-loader": "7.1.1",
    "body-parser": "1.17.2",
    "brace-expansion": "1.1.8",
    "convict": "0.8.2",
    "css-loader": "0.28.4",
    "express": "4.15.3",
    "extract-text-webpack-plugin": "2.1.2",
    "node-sass": "4.5.3",
    "react": "15.6.1",
    "react-dom": "15.5.4",
    "prop-types": "15.5.8",
    "sass-loader": "6.0.6",
    "webpack": "3.3.0",
    "winston": "2.3.1"
  },
  "scripts": {
    "prestart": "webpack",
    "start": "node src/app.js"
  },
  "devDependencies": {
    "webpack-dev-server": "1.10.1",
    "babel-core": "6.24.1",
    "babel-register": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-loader": "7.0.0",
    "babel-plugin-rewire": "1.1.0",
    "babel-preset-stage-3": "6.24.1",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-react-hmre": "1.1.1",
    "babel-polyfill": "6.23.0"
  }
}
